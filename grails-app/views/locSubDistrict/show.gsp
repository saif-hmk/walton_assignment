                            <g:link class="btn btn-xs btn-block btn-danger confirm-delete" action="delete" id="${locSubDistrictInstance?.id}">%{-- onclick="return confirm('Are you sure you want to delete this record?');">--}%
                                <i class="fa fa-trash"></i> <g:message code="default.button.delete" default="Delete"/>
                            </g:link>
                        </li>
                    </sec:access>
                </ul>
